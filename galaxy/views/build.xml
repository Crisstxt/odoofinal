<odoo>
    <data>

    <record model="ir.ui.view" id="galaxy.buildform">
      <field name="name">estructura form</field>
      <field name="model">galaxy.building</field>
      <field name="arch" type="xml">
          <form>
            <sheet>
              <group>
                <field name="name"/>
                <field name="description"/>
                <field name="picture" widget="image"/>
                <field name="population"/>
                <field name="precio"/>
                <field name="tipobuild"/>               
              </group>
            </sheet>
          </form>
      </field>
    </record>

    <record model="ir.ui.view" id="galaxy.buildtree">
      <field name="name">estructura tree</field>
      <field name="model">galaxy.building</field>
      <field name="arch" type="xml">
          <tree>
                <field name="name"/>
                <field name="description"/>
                <field name="picture" widget="image"/>
                <field name="population"/>
                <field name="precio"/>  
                <field name="tipobuild"/>        
          </tree>
      </field>
    </record>

    <record model="ir.ui.view" id="galaxy.buildtypeform">
      <field name="name">estructura type form</field>
      <field name="model">galaxy.building_type</field>
      <field name="arch" type="xml">
          <form>
            <sheet>
              <group>
                
              </group>
            </sheet>
          </form>
      </field>
    </record>

    <record model="ir.ui.view" id="galaxy.buildtypetree">
      <field name="name">estructura type tree</field>
      <field name="model">galaxy.building_type</field>
      <field name="arch" type="xml">
          <tree>
       
          </tree>
      </field>
    </record>

    </data>
</odoo>    